set number
set mouse=a
set numberwidth=1
set clipboard=unnamed
syntax on
set showcmd
set ruler
set cursorline
set encoding=utf-8
set showmatch
set sw=2
set relativenumber
set laststatus=2
set noshowmode
set cursorline
set t_Co=256

"" Searching
set hlsearch
set incsearch
set ignorecase
set smartcase

so ~/.vim/maps.vim
so ~/.vim/plugins.vim
so ~/.vim/plugin-config.vim

set background=dark
set termguicolors

colorscheme gruvbox

"let g:gruvbox_contrast_dark='hard'
"highlight Normal ctermbg=NONE
"

"let g:coc_global_extension = ['coc-json', 'coc-tsserver']
